{
  "name": "jspicl",
  "version": "2.4.1",
  "description": "A javascript to pico-8 lua converter",
  "main": "lib/jspicl.js",
  "module": "src/index.js",
  "scripts": {
    "prestart": "rm -rf build",
    "start": "rollup -c",
    "prepublishOnly": "npm test && npm start",
    "tester": "NODE_PATH=./src node -r esm tester.js",
    "test": "NODE_ENV=test NODE_PATH=./src mocha",
    "codecoverage": "nyc npm t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgronKabashi/jspicl"
  },
  "keywords": [
    "pico-8",
    "javascript",
    "buildtool",
    "lua",
    "ast",
    "transpile",
    "transpiler"
  ],
  "author": "Agron Kabashi",
  "license": "MIT",
  "devDependencies": {
    "@agronkabashi/eslint-config": "^1.0.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "deepmerge": "^2.1.1",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rollup": "^1.15.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.0.1",
    "sinon": "^7.3.2"
  },
  "esm": {
    "mode": "all",
    "cjs": {
      "paths": true
    }
  },
  "dependencies": {
    "esprima": "^4.0.1",
    "trastpiler": "^1.0.1"
  },
  "engines": {
    "node": ">=6"
  }
}
